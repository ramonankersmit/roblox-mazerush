{
  "name": "MazeRush",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$path": "src/ReplicatedStorage",
      "$ignoreUnknownInstances": true,
      "Modules": {
        "$className": "Folder",
        "$path": "src/ReplicatedStorage/Modules"
      },
      "Remotes": {
        "$className": "Folder",
        "RoundState": {
          "$className": "RemoteEvent"
        },
        "Countdown": {
          "$className": "RemoteEvent"
        },
        "Pickup": {
          "$className": "RemoteEvent"
        },
        "DoorOpened": {
          "$className": "RemoteEvent"
        },
        "SetMazeAlgorithm": {
          "$className": "RemoteEvent"
        },
        "SetLoopChance": {
          "$className": "RemoteEvent"
        },
        "InventoryUpdate": {
          "$className": "RemoteEvent"
        },
        "LobbyState": {
          "$className": "RemoteEvent"
        },
        "ToggleReady": {
          "$className": "RemoteEvent"
        },
        "StartGameRequest": {
          "$className": "RemoteEvent"
        },
        "ThemeVote": {
          "$className": "RemoteEvent"
        }
      },
      "State": {
        "$className": "Folder",
        "MazeAlgorithm": {
          "$className": "StringValue",
          "$properties": {
            "Value": "DFS"
          }
        },
        "LoopChance": {
          "$className": "NumberValue",
          "$properties": {
            "Value": 0.05
          }
        },
        "Difficulty": {
          "$className": "StringValue",
          "$properties": {
            "Value": "Gemiddeld"
          }
        },
        "Phase": {
          "$className": "StringValue",
          "$properties": {
            "Value": "IDLE"
          }
        },
        "Theme": {
          "$className": "StringValue",
          "$properties": {
            "Value": "Spooky"
          }
        }
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$path": "src/ServerScriptService",
      "$ignoreUnknownInstances": true
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",

      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "$path": "src/StarterPlayer/StarterPlayerScripts",
        "$ignoreUnknownInstances": true
      }
    },

    "StarterGui": {
      "$className": "StarterGui",
      "$path": "src/StarterGui",
      "$ignoreUnknownInstances": true
    },

    "Workspace": {
      "$className": "Workspace",
      "$path": "src/Workspace",
      "$ignoreUnknownInstances": true,

      "Lobby": {
        "$className": "Folder",
        "$path": "src/Workspace/Lobby",
        "$ignoreUnknownInstances": true
      }
    }
  }
}
